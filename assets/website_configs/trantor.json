{
    "name": "trantor",
    "categories": [
        "ebooks"
    ],
    "search": {
        "url": "https://trantor.is/search/?q=",
        "spaceReplacement": "+",
        "itemKeys": {
            "root": ".card",
            "name": ".card-title strong",
            "thumbnail": {
                "key": "img",
                "attribute": "src",
                "appendToSiteUrl": true
            },
            "link": ".card-title a",
            "metadata": {
                "language": ".card-title span:nth-of-type(1)",
                "publisher": ".card-title span:nth-of-type(2)",
                "author": ".card-text .col-md-9"
            }
        }
    }
}