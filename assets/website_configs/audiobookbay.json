{
    "name": "audiobookbay",
    "categories": [
        "audio_books"
    ],
    "search": {
        "url": "https://audiobookbay.li/?s=",
        "spaceReplacement": "+",
        "itemKeys": {
            "root": ".post",
            "name": ".postTitle a",
            "thumbnail": {
                "key": "img",
                "attribute": "src"
            },
            "link": ".posttitle a",
            "metadata": {
                "format": ".postContent p:nth-of-type(2) span:nth-of-type(1)",
                "bitrate": ".postContent p:nth-of-type(2) span:nth-of-type(2)"
            }
        }
    }
}